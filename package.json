{
  "name": "dannbox-docs",
  "version": "1.0.0",
  "description": "Dannbox documentation with Nextra",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "pagefind --site .next/server/app --output-path public/_pagefind",
    "start": "next start",
    "lint": "next lint",
    "docker:build": "docker build -t dannbox-docs .",
    "docker:run": "docker run -p 3000:3000 dannbox-docs",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.0.0",
    "next": "^16.1.6",
    "nextra": "^4.6.1",
    "nextra-theme-docs": "^4.6.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "pagefind": "^1.3.0",
    "typescript": "^5"
  }
}
